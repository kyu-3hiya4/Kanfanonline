<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>

  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"ユーザーネーム"%>

  <label for="inputEmail">Email</label>
  <%= f.email_field :email, placeholder: "メールアドレス" %>  
  
  <%= f.submit "変更を保存" %>
<% end %>