<h2>検索結果</h2>

<table>
   <% if @model == 'user' %>
<tbody>
  <% @users.each do |user| %> <tr>
    <td><%= user.name %> </td> <tr>
    <% end %>
</tbody>
<% else %>

<tbody>
  <% @posts.each do |post| %> <tr>
    <td><%= post.title %> </td>
    <td><%= post.body %> </td> <tr>
  <% end %>
  </tbody>
  <% end %>
  </table>

 
